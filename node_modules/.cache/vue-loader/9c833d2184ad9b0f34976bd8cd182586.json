}, \n                required: false, \n                default: 'H'\n            }\n        },\n\n        watch: {\n            text: function () {\n                this.clear();\n                this.makeCode(this.text);\n            }\n        },\n\n        data() {\n            return{\n                qrCode: {}\n            }\n        },\n\n        mounted() {\n            this.qrCode = new QRCode(this.$el, {\n                text: this.text,\n                width: this.size,\n                height: this.size,\n                colorDark : this.color,\n                colorLight : this.bgColor,\n                correctLevel : QRCode.CorrectLevel[this.errorLevel]\n            });\n        },\n\n        methods: {\n            clear: function () {\n                this.qrCode.clear();\n            },\n            makeCode: function (text) {\n                this.qrCode.makeCode(text);\n            }\n        }\n    }\n</script>"]}]}